<h1 class="text-center">Results</h1>

<div class="container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">

      <h2 class="text-center">Initial text</h2>
      <p>
        <%= @raw_message %>
      </p>
      <ol>
        <% @choices.each do |choice| %>
          <li><%= choice %></li>
        <% end %>
      </ol>

    </div>
  </div>
</div>
